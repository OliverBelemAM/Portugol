algoritmo "Procedimentos-5"
var
v: inteiro

   procedimento parouimpar (n,)
   inicio
         se (n % 2 = 0) entao
            escreva ("O numero digitado é PAR")
         senao
            escreva ("O numero digitado é IMPAR")
         fimse
   fimprocedimento
   
inicio
      escreva (" Digite um valor: ")
       leia (v)
      parouimpar (v)
fimalgoritmo
