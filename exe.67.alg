algoritmo "exe.67"
var
   cont, num : inteiro
inicio

      escreva (" Vamos contar até: ")
         leia (num)
         
              limpatela
              
         escreva (" Contagem:")
         
      para cont <- 0 ate num faca
           escreva (cont)
      fimpara
      
      escreva (" FIM! ")
fimalgoritmo
